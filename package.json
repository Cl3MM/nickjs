{
  "name": "nickjs",
  "version": "0.0.11",
  "description": "Nearly headless Nick",
  "homepage": "https://nickjs.org/",
  "license": "ISC",
  "main": "lib/Nick.js",
  "dependencies": {
    "babel-runtime": "6.23.0",
    "bluebird": "3.5.0",
    "chrome-remote-interface": "^0.23.3",
    "lodash": "4.17.4",
    "needle": "^1.6.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-polyfill": "6.23.0",
    "babel-plugin-lodash": "3.2.11",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.3.3"
  },
  "scripts": {
    "test": "echo \"Error: no test exist\" && exit 1",
    "build": "babel src -d lib",
    "build:watch": "npm run build -- --watch",
    "build-and-fix-casper": "babel src -d lib && sed -i '1s/^/var require = patchRequire(require);/' lib/casper/TabDriver.js",
    "build-and-fix-casper:watch": "while true; do inotifywait -e close_write src/*.js src/*/*.js; npm run build-and-fix-casper; done",
    "build-examples-casper": "babel examples-casper/src -d examples-casper/lib",
    "build-examples-casper:watch": "npm run build-examples-casper -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phantombuster/nickjs.git"
  },
  "author": {
    "name": "Martin Tapia",
    "email": "martin@phantombuster.com",
    "url": "https://phantombuster.com"
  },
  "maintainers": [
    {
      "name": "Martin Tapia",
      "email": "martin@phantombuster.com",
      "url": "https://phantombuster.com"
    }
  ],
  "bugs": {
    "url": "https://github.com/phantombuster/nickjs/issues",
    "email": "martin@phantombuster.com"
  },
  "keywords": [
    "nick",
    "nickjs",
    "chrome",
    "headless",
    "phantombuster",
    "capserjs",
    "phantomjs"
  ]
}
